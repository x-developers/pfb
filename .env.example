# Example .env file for Pump Fun Bot - Ultra-Fast Mode Only
# Copy this to .env and fill in your actual values

# ========================================
# REQUIRED SETTINGS
# ========================================

# Your Solana wallet private key (base58 encoded)
# Get this from your wallet (Phantom, Solflare, etc.)
PUMPBOT_PRIVATE_KEY=your_private_key_here

# ========================================
# NETWORK CONFIGURATION
# ========================================

# Network to use (mainnet or devnet)
PUMPBOT_NETWORK=devnet

# Custom RPC endpoint (optional - leave empty for defaults)
# Examples:
# - Mainnet: https://api.mainnet-beta.solana.com
# - Devnet: https://api.devnet.solana.com
# - QuickNode: https://your-endpoint.solana-mainnet.quiknode.pro/your-key/
# - Alchemy: https://solana-mainnet.g.alchemy.com/v2/your-key
PUMPBOT_RPC_URL=

# Custom WebSocket endpoint (optional - leave empty for defaults)
# Examples:
# - Mainnet: wss://api.mainnet-beta.solana.com
# - Devnet: wss://api.devnet.solana.com
# - QuickNode: wss://your-endpoint.solana-mainnet.quiknode.pro/your-key/
PUMPBOT_WS_URL=

# RPC API Key (if your provider requires it)
PUMPBOT_RPC_API_KEY=

# ========================================
# TRADING CONFIGURATION
# ========================================

# Amount to spend per trade in SOL
PUMPBOT_TRADING_BUY_AMOUNT_SOL=0.01

# Slippage tolerance in basis points (500 = 5%)
PUMPBOT_TRADING_SLIPPAGE_BP=1000

# Priority fee for transactions (0 = default)
PUMPBOT_TRADING_PRIORITY_FEE=10000

# Auto-sell settings
PUMPBOT_TRADING_AUTO_SELL=false
PUMPBOT_TRADING_SELL_DELAY_SECONDS=30
PUMPBOT_TRADING_SELL_PERCENTAGE=100.0

# ========================================
# üïí TIMING CONTROLS
# ========================================

# Maximum age of token before rejecting (milliseconds)
# Set to 0 to disable age checking
# Recommended: 3000-8000ms for fast trading, 10000+ for safe trading
PUMPBOT_TRADING_MAX_TOKEN_AGE_MS=5000

# Minimum delay after token discovery before allowing trades (milliseconds)
# Set to 0 to disable minimum delay
# Recommended: 50-200ms to avoid immediate rush, 0 for maximum speed
PUMPBOT_TRADING_MIN_DISCOVERY_DELAY_MS=100

# ========================================
# STRATEGY CONFIGURATION
# ========================================

# Strategy type (sniper or holder)
PUMPBOT_STRATEGY_TYPE=sniper

# YOLO mode (continuous trading - DANGEROUS!)
PUMPBOT_STRATEGY_YOLO_MODE=false

# Hold only mode (buy but never sell)
PUMPBOT_STRATEGY_HOLD_ONLY=true

# Maximum tokens to trade per hour
PUMPBOT_STRATEGY_MAX_TOKENS_PER_HOUR=5

# Token filtering
PUMPBOT_STRATEGY_FILTER_BY_NAME=false
PUMPBOT_STRATEGY_FILTER_BY_CREATOR=false

# ========================================
# üõ°Ô∏è JITO MEV PROTECTION
# ========================================

# Enable JITO MEV protection
PUMPBOT_JITO_ENABLED=false

# JITO Block Engine endpoint
# Mainnet endpoints by region:
# - Default: https://mainnet.block-engine.jito.wtf/api/v1/bundles
# - Frankfurt: https://frankfurt.mainnet.block-engine.jito.wtf/api/v1/bundles
# - Amsterdam: https://amsterdam.mainnet.block-engine.jito.wtf/api/v1/bundles
# - NY: https://ny.mainnet.block-engine.jito.wtf/api/v1/bundles
# - Tokyo: https://tokyo.mainnet.block-engine.jito.wtf/api/v1/bundles
PUMPBOT_JITO_ENDPOINT=https://mainnet.block-engine.jito.wtf/api/v1/bundles

# JITO API key (if required by your provider)
PUMPBOT_JITO_API_KEY=

# Tip amount for MEV protection (lamports)
# 10000 = 0.00001 SOL, 50000 = 0.00005 SOL
# Higher tips = better MEV protection but higher costs
PUMPBOT_JITO_TIP_AMOUNT=10000

# Use JITO for trading transactions (buy/sell)
PUMPBOT_JITO_USE_FOR_TRADING=false

# Bundle confirmation timeout (seconds)
PUMPBOT_JITO_CONFIRM_TIMEOUT=30

# Maximum transactions per bundle
PUMPBOT_JITO_MAX_BUNDLE_SIZE=5

# ========================================
# ‚ö°‚ö° ULTRA-FAST MODE (DEFAULT MODE)
# ========================================

# Ultra-fast mode is always enabled - configure performance settings below

# Skip validation checks for maximum speed (DANGEROUS!)
PUMPBOT_ULTRA_FAST_SKIP_VALIDATION=false

# Don't wait for transaction confirmation
PUMPBOT_ULTRA_FAST_NO_CONFIRMATION=false

# Fire-and-forget mode (send and continue immediately)
PUMPBOT_ULTRA_FAST_FIRE_AND_FORGET=false

# Number of parallel processing workers (1-10)
# Recommended: 3-5 for optimal performance
PUMPBOT_ULTRA_FAST_PARALLEL_WORKERS=3

# Queue buffer size for token processing
PUMPBOT_ULTRA_FAST_TOKEN_QUEUE_SIZE=100

# Cache blockhash for speed
PUMPBOT_ULTRA_FAST_CACHE_BLOCKHASH=true

# Blockhash refresh interval (seconds)
PUMPBOT_ULTRA_FAST_BLOCKHASH_INTERVAL=10

# Pre-compute instructions for speed
PUMPBOT_ULTRA_FAST_PRECOMPUTE_INSTRUCTIONS=true

# RPC timeout in milliseconds
PUMPBOT_ULTRA_FAST_RPC_TIMEOUT_MS=3000

# Enable detailed latency logging
PUMPBOT_ULTRA_FAST_LOG_LATENCY=false

# Enable benchmark mode
PUMPBOT_ULTRA_FAST_BENCHMARK_MODE=false

# Maximum tokens per second (rate limiting)
PUMPBOT_ULTRA_FAST_MAX_TOKENS_PER_SECOND=10

# Emergency stop loss percentage
PUMPBOT_ULTRA_FAST_EMERGENCY_STOP_LOSS=-50.0

# Use fixed buy amounts for speed
PUMPBOT_ULTRA_FAST_FIXED_BUY_AMOUNT=true

# Skip ATA creation checks (DANGEROUS!)
PUMPBOT_ULTRA_FAST_SKIP_ATA_CREATION=false

# Assume ATA already exists (DANGEROUS!)
PUMPBOT_ULTRA_FAST_ASSUME_ATA_EXISTS=false

# Execute immediately without delay
PUMPBOT_ULTRA_FAST_IMMEDIATE_EXECUTION=true

# Priority over safety (EXTREMELY DANGEROUS!)
PUMPBOT_ULTRA_FAST_PRIORITY_OVER_SAFETY=false

# ========================================
# LOGGING CONFIGURATION
# ========================================

# Log level (debug, info, warn, error)
PUMPBOT_LOGGING_LEVEL=info

# Log format (text or json)
PUMPBOT_LOGGING_FORMAT=text

# File logging
PUMPBOT_LOGGING_LOG_TO_FILE=false
PUMPBOT_LOGGING_LOG_FILE_PATH=logs/bot.log
PUMPBOT_LOGGING_TRADE_LOG_DIR=trades

# ========================================
# ADVANCED SETTINGS
# ========================================

# Transaction settings
PUMPBOT_ADVANCED_MAX_RETRIES=3
PUMPBOT_ADVANCED_RETRY_DELAY_MS=1000
PUMPBOT_ADVANCED_CONFIRM_TIMEOUT_SEC=30

# Metrics
PUMPBOT_ADVANCED_ENABLE_METRICS=false
PUMPBOT_ADVANCED_METRICS_PORT=8080

